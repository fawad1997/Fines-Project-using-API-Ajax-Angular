@{ ViewBag.Title = "Index"; }
<h2>Crud Operation using Web API, Ajax, Json and Angular</h2>
@Scripts.Render("~/bundles/jquery")
<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/angular.min.js"></script>
<script src="~/Scripts/Angular/myAngular.js"></script>

<div ng-app="Homeapp">
    <div ng-controller="MyFirstController">
        <label>Enter Box Hight/Width</label>
        <input type="text" ng-model="hw" />
        <my-custom-color-list></my-custom-color-list>
        <input type="radio" ng-model="gender" value="Male" />Male
        <input type="radio" ng-model="gender" value="Female" />Female
        <br />
        Skills:
        <input type="checkbox" ng-model="php" />Php
        <input type="checkbox" ng-model="angular" />Angular
        <div style="height:{{hw}}px;width:{{calculateWidth()}}px;background-color:{{color}};"></div>
        <ol>
            <li ng-repeat="x in countries">{{x}}</li>
        </ol>
        {{"Your gender is "+gender}}
        {{"Your skills are php->"+php+", angular->"+angular}}
    </div>
    {{msg}}
</div>